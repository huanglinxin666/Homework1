#include <iostream>
using namespace std;
int main(){
 cout << "输入10个数" << endl;
 int num_1,num_2,num_3,num_4,num_5,num_6,num_7,num_8,num_9,num_10;
 int num[10] = { num_1,num_2,num_3,num_4,num_5,num_6,num_7,num_8,num_9,num_10};
 
 scanf("%d%d%d%d%d%d%d%d%d%d",&num_1,&num_2,&num_3,&num_4,&num_5,&num_6,&num_7,&num_8,&num_9,&num_10 );
 printf("num_10 = %d,\nnum_9 = %d,\nnum_8 = %d,\nnum_7 = %d,\nnum_6 = %d,\nnum_5 = %d,\nnum_4 = %d,\nnum_3 = %d,\nnum_2 = %d,\nnum_1 = %d\n",num_10,num_9,num_8,num_7,num_6,num_5,num_4,num_3,num_2,num_1);
 
 return 0;
}
